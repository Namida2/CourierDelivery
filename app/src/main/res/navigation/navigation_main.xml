<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_main"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/routesMapsFragment"
        android:name="com.example.courierdelivery.views.fragments.RoutesMapsFragment"
        android:label="RoutesMapsFragment" >
        <action
            android:id="@+id/action_routesMapsFragment_to_mapFragment"
            app:destination="@id/mapFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.courierdelivery.views.fragments.MapFragment"
        android:label="MapFragment" >
        <action
            android:id="@+id/action_mapFragment_to_routesMapsFragment"
            app:destination="@id/routesMapsFragment" />
    </fragment>
</navigation>